if (CC == 416	)	MsgId_1.Country	=	"Taiwan"  ;else
if (CC == 472	)	MsgId_1.Country	=	"Tajikistan"  ;else
if (CC == 674|| CC== 677 	)	MsgId_1.Country	=	"Tanzania" ;else
if (CC == 567	)	MsgId_1.Country	=	"Thailand" ;else
if (CC == 671	)	MsgId_1.Country	=	"Togolese"  ;else
if (CC == 570	)	MsgId_1.Country	=	"Tonga" ;else
if (CC == 362	)	MsgId_1.Country	=	"Trinidad" ;else
if (CC == 672	)	MsgId_1.Country	=	"Tunisia" ;else
if (CC == 271	)	MsgId_1.Country	=	"Turkey" ;else
if (CC == 434	)	MsgId_1.Country	=	"Turkmenistan" ;else
if (CC == 364	)	MsgId_1.Country	=	"Turks and Caicos" ;else
if (CC == 572	)	MsgId_1.Country	=	"Tuvalu" ;else
if (CC == 675	)	MsgId_1.Country	=	"Uganda";else
if (CC == 272	)	MsgId_1.Country	=	"Ukraine" ;else
if (CC == 470 || CC== 471 	)	MsgId_1.Country	= "Arab Emirates" ;else
if (CC == 232 || CC== 233|| CC== 234|| CC== 235 	)	MsgId_1.Country	=	"Great Britain";else
if (CC == 379	)	MsgId_1.Country	=	 "Virgin Islands" ;else
if (CC == 338 || CC== 366|| CC== 367|| CC== 368|| CC== 369 	)	MsgId_1.Country	= "America" ;else
if (CC == 770	)	MsgId_1.Country	=	"Uruguay";else
if (CC == 437	)	MsgId_1.Country	=	"Uzbekistan" ;else
if (CC == 576 || CC== 577 	)	MsgId_1.Country	=	"Vanuatu";else
if (CC == 208	)	MsgId_1.Country	=	"Vatican";else
if (CC == 775	)	MsgId_1.Country	=	"Venezuela" ;else
if (CC == 574	)	MsgId_1.Country	=	"Vietnam" ;else
if (CC == 578	)	MsgId_1.Country	=	"Wallis";else
if (CC == 473 || CC== 475 )	MsgId_1.Country	=	"Yemen" ;else
if (CC == 678	)	MsgId_1.Country	=	"Zambia";else
if (CC == 679	)	MsgId_1.Country	=	"Zimbabwe"; else MsgId_1.Country	=	"--";
return MsgId_1.Country;
}

// #   Esas oglan burda! Asıl is burdan sonra...      #

int GetTheShitDone(string line){
ln.clear();					// -> NMEA Sentence is assigned to that variable
subArray.clear();
char FragmentIndicator='1';
t384.set(0);
t168.set(0);
								// -> That was the problematic area, that is causing same result to be returned...
	len = line.length();
													// -> ############################################################################# <-
	istringstream iss(line);						//
	string part="";									// Field 1(0) -> !AIVDM -> Identifies the Package Type
	int l=0;										// Field 2(1) -> Fragment Count of the message since it may take longer then 28 char
	while(getline(iss,part,','))
	{												// Field 3(2) -> Fragment Number of the Message
		subArray.push_back(part);					// Field 4(3) -> Sequence Nr for multi part sentences
		l++;										// Field 6(5) -> Payload...
	}												// Field 7(6) -> Bit requirements to pad the payload
													// Field 8(7) -> Checksum recieved with the message for quality check.
													// -> ############################################################################# <-
FragmentIndicator=subArray[2][0];
cout << "Fragmen Indicator " << FragmentIndicator << endl;
// # Dividing is Complete... Procceding with CHECKSUM  #



	if (!GetTheCheckSumResult(line)){					//checksum control!... Dont know if that is the perfect way to do it
		//cout << "Bad Message" << endl;									//although i have a function on top.. ???
	}

	else{
		//cout << "Message being processed..." << endl;





// #     Control Point For Message Type 1-3 / 5 / 9    #

	 if (subArray[5][0]=='1' || subArray[5][0]== '2' || subArray[5][0]== '3') {

		//cout << "#####################" << endl;
		//cout << "Message Type : 1 to 3" << endl;
		//cout << "#####################" << endl;


							// #  First thing First : Binarize the entire message  #


							string txt = subArray[5];							//
							bitCnt=167;											//
							for(unsigned i = 0; i < txt.length(); i++)			//
							   {												//
									int Decml =(int)txt[i] -48;					// 6 bit extraction
								   if ( Decml > 40)								//
								   {											// Decml is the final result of
									   Decml-=8;								// 6 bit version of chars in msg
								   }											//

							byt =bitset<6>(Decml);								//
								for (int k = 5; k>=0;k--)						//
								{												// Assigning each bit to Array "t168"
									t168[bitCnt]=byt[k];						//
									bitCnt--;}									//
							   }												//

							// #  At this point "t168" has all the bits we need    #

							// #  Procceding with Talker ID (Kind Of Message Type) #


							TalkerID = GetTalkerID(subArray[0].substr(1,2));


							// #      Parsing the First Message Type (1-2-3)       #	// Combining the BITs for Message Type 1,2 and 3

							int MsgType			=  get6BitVal(167,5);
							MsgId_1.MMSI 		=  get6BitVal(159,29);
							MsgId_1.Country		=  get6BitVal(159,17);					//				//
							MsgId_1.Spd			= (get6BitVal(117,9))/10;				//
							MsgId_1.longtitude	=  get6BitVal(106,27);					//
							MsgId_1.latitude	=  get6BitVal(78,26);					//
							MsgId_1.GHeading	= (get6BitVal(51,11))/10;				// Course Over Ground
							MsgId_1.THeading	=  get6BitVal(39,8);
if (MsgId_1.THeading == 511)
MsgId_1.THeading = 0;
							// #       Parsing the First Message [ D O N E ]       #

							// #     Convert Coordinates to something readable     #

							//****************************************************************************************************************************

							// #   Convert the Latitude and Longtitude Shit        #			// This portion will also be same for all types of messages...


							MsgId_1.lastlong	= 	ConvertToLongtitude	(MsgId_1.longtitude);
							MsgId_1.lastlat		=	ConvertToLatitude	(MsgId_1.latitude);


							// #         Get The Country Code From MMSI            #			// This portion will be same for all types of messages...

							MsgId_1.Country = GetTheCountryName(MsgId_1.MMSI/1000000);


							// #             Proceeding with Show-Off              #

							cout << "Message Type\t: "		<< "1 / 2 / 3" << endl;
							cout << "Mesaj Numarası\t: " 	<< MsgType << endl;
							cout << "Mesage Provider\t: " 	<< TalkerID << endl;							//*****************************************
							cout << "Environment\t: "		<< "Surface" << endl;
							cout << "MMSI Nr\t\t: "			<< dec << MsgId_1.MMSI << endl;					// How will i solve this problem, damn it!
							cout << "Country\t\t: " 		<< MsgId_1.Country << endl;						//*****************************************
							cout << "Speed\t\t: "	 		<< MsgId_1.Spd<< endl;							//
							cout << "Longtitude\t: " 		<< MsgId_1.lastlong<< endl;						//
							cout << "Latitude\t: " 			<< MsgId_1.lastlat<< endl;						//
							cout << "Heading\t\t: "	 		<< MsgId_1.GHeading<< endl;						// Course Over Ground
							cout << "True Heading\t: "		<< MsgId_1.THeading<< endl;						// True Heading

}

	else if (subArray[5][0]== '5' && FragmentIndicator == '1'){
// -> That was the problematic area, that is causing same result to be returned...

	//cout << "################" << endl;
	//cout << "Message Type : 5" << endl;
	//cout << "################" << endl;
							// #  First thing First : Binarize the entire message  #


							string txt = subArray[5];							//
							bitCnt=383;
									//
							for(unsigned i = 0; i < txt.length(); i++)			//
							   {												//
									int Decml =(int)txt[i] -48;					// 6 bit extraction
								   if ( Decml > 40)								//
								   {											// Decml is the final result of
									   Decml-=8;								// 6 bit version of chars in msg
								   }											//

							byt =bitset<6>(Decml);
								for (int k = 5; k>=0;k--)						//
								{												// Assigning each bit to Array "t168"
									t384[bitCnt]=byt[k];
									bitCnt--;}
							   }												//
						TalkerID = GetTalkerID(subArray[0].substr(1,2));


							// #      Parsing the First Message Type (1-2-3)       #	// Combining the BITs for Message Type 1,2 and 3
							int MsgType			=  get6BitVal5(383,5);
							MsgId_5.MMSI 		=  get6BitVal5(375,29);
							MsgId_5.CallSign	=  get6BitChar(313,42);					//
							MsgId_5.ShipName	=  get6BitChar(271,119);				//

							cout << "Message Type\t: "		<< "5" << endl;
							cout << "Msg Numarası \t: "		<< MsgType << endl;
							cout << "Mesage Provider\t: " 	<< TalkerID << endl;							//*****************************************
							cout << "Environment\t: "		<< "Surface" << endl;
							cout << "MMSI Nr\t\t: "			<< dec << MsgId_5.MMSI << endl;					// How will i solve this problem, damn it!
							cout << "CallSign\t: " 			<< MsgId_5.CallSign<< endl;
							cout << "Ship Name\t: "			<< MsgId_5.ShipName << endl;

}
else if (subArray[5][0]== '9'){
								// -> That was the problematic area, that is causing same result to be returned...

								// #  First thing First : Binarize the entire message  #


							string txt = subArray[5];							//
							bitCnt=167;											//
							for(unsigned i = 0; i < txt.length(); i++)			//
							   {												//
									int Decml =(int)txt[i] -48;					// 6 bit extraction
								   if ( Decml > 40)								//
								   {											// Decml is the final result of
									   Decml-=8;								// 6 bit version of chars in msg
								   }											//

							byt =bitset<6>(Decml);								//
								for (int k = 5; k>=0;k--)						//
								{												// Assigning each bit to Array "t168"
									t168[bitCnt]=byt[k];						//
									bitCnt--;}									//
							   }												//

							// #  At this point "t168" has all the bits we need    #

							// #  Procceding with Talker ID (Kind Of Message Type) #


							TalkerID = GetTalkerID(subArray[0].substr(1,2));


							// #      Parsing the First Message Type (1-2-3)       #	// Combining the BITs for Message Type 1,2 and 3


							MsgId_9.MMSI 		=  get6BitVal(159,29);
							MsgId_9.Altitude	=  get6BitVal(129,11);					//
							MsgId_9.Spd			= (get6BitVal(117,9));				//
							MsgId_9.longtitude	=  get6BitVal(106,27);					//
							MsgId_9.latitude	=  get6BitVal(78,26);					//
							MsgId_9.GHeading	= (get6BitVal(51,11))/10;				//



							// #       Parsing the First Message [ D O N E ]       #

							// #     Convert Coordinates to something readable     #

							//****************************************************************************************************************************

							// #   Convert the Latitude and Longtitude Shit        #			// This portion will also be same for all types of messages...


							MsgId_9.lastlong	= ConvertToLongtitude	(MsgId_9.longtitude);
							MsgId_9.lastlat		= ConvertToLatitude		(MsgId_9.latitude);


							// #         Get The Country Code From MMSI            #			// This portion will be same for all types of messages..

							// #             Proceeding with Show-Off              #

							cout << "Message Type\t: "		<< "9" << endl;
							cout << "Mesage Provider\t: " 	<< TalkerID << endl;							//*****************************************
							cout << "Environment\t: "		<< "Air" << endl;								// This is due to message type...
							cout << "MMSI Nr\t\t: "			<< dec << MsgId_9.MMSI << endl;					// How will i solve this problem, damn it!
							cout << "Speed\t\t: "	 		<< MsgId_9.Spd<< endl;
							cout << "Altitude\t: "			<< MsgId_9.Altitude << endl; 				//*****************************************
							cout << "Longtitude\t: " 		<< MsgId_9.lastlong<< endl;						//
							cout << "Latitude\t: " 			<< MsgId_9.lastlat<< endl;						//
							cout << "Heading\t\t: "	 		<< MsgId_9.GHeading<< endl;						// Course Over Ground

}
else{

	cout << "Not relevant NMEA Sentence..." << endl;
}
}
fflush(stdin);
fflush(stdout);
return 0;
}

