char ais2ascii( char value ){
     value = value & 0x3F;
     if( value < 0x20 )
         return value + 0x40;
     else
         return value;
}
bool GetTheCheckSumResult(string csum){					// Getting the CheckSum result...

/*	if (csum.length()-1>4){
		cout << "Bad Message" << endl;
		return false;
	}

	else{
		cout << "Message being processed..." << endl;
		return true;
	}*/
	istringstream n(csum);
	string sentence;									// There is somthing wrong with that checksum...
	string received_checksum;

	getline(n, sentence, '*');
	getline(n, received_checksum);

	char check = accumulate(sentence.begin()+1, sentence.end(), 0,[](char sum, char ch) { return sum ^ ch; });

		//cout << "**********************"<<endl;
		//cout << "received checksum: " << received_checksum<<endl;			// -> ############################################ <-
		//cout << "computed checksum: " << hex << (int)check << endl;			// -> #  Just a Test Line! Can be removed Later  # <-
		//cout << "**********************"<<endl<< endl;																	// -> ############################################ <-

	ostringstream ss;
	ss << hex << (int)check;
	string res = ss.str();

	if (res != received_checksum){
		cout << "-----------------------------------" << endl;
		cout << "Message Integrity check  [ PASSED ]" << endl;
		cout << "-----------------------------------" << endl;
		return true;
	}																		// -> ####################################################### <-
	else{
		cout << "-----------------------------------" << endl;				// -> # Instead of Fancy Msg, we will BREAK the Function!!! # <-
		cout << "Message Integrity check  [ FAILED ]" << endl;
		cout << "-----------------------------------" << endl;
	return true;
																			// -> ####################################################### <-
	}
}
unsigned int get6BitVal(int cntr, int NrOfBits){ 		// Will return bit partial assinments of entire sentence PayLoad.
	int result=0;										// to convert those 0 and 1 s into sth understandable...
	for (int j = 0; j<=NrOfBits ; j++)
	{
		result *= 2;
		result += t168[cntr];
		cntr--;
	}
	//if (NrOfBits % 5 == 0) cout << "Printing the 6 bit number : " << result << endl;

	return result;
}
unsigned int get6BitVal5(int cntr, int NrOfBits){ 		// Will return bit partial assinments of entire sentence PayLoad.
	int result=0;										// to convert those 0 and 1 s into sth understandable...
	for (int j = 0; j<=NrOfBits ; j++)
	{
		result *= 2;
		result += t384[cntr];
		cntr--;
	}

	return result;
}
string get6BitChar(int cntr, int NrOfBits){ 			// Will return bit partial assinments of entire sentence PayLoad.
unsigned int result;
ostringstream name;
string res;
int j= 0;
	for (int i = 0; i<NrOfBits ; i=i+6)
	{
		result = 0;
		for (j = 0; j <=5;j++)
		{
		result *= 2;
		result += t384[cntr-(i+j)];
		//cntr--;
		}
		name<<ais2ascii(result);
	}
res = name.str();
return res;
}
string ConvertToLongtitude(long &longtitude){			// Converting the Longtitude to sth we can read
    if( longtitude & 0x8000000 )
    {
        longtitude = 0x10000000 - longtitude;     //Converted to Signed Integer
        longtitude *= -1;
    }

    double long_ddd = (int) (longtitude / 600000);
         long_ddd += (double) (longtitude - (long_ddd * 600000)) / 600000.0;
		 long_ddd = (short) (longtitude / 600000);
    double long_min = abs((longtitude - (long_ddd * 600000))) / 10000.0;
char v;
	if (long_ddd>0)
		v='E';
	else
		v='W';



ostringstream longfinal;
longfinal << long_ddd << " " << long_min << " " << v;
return longfinal.str();

	 }
string ConvertToLatitude(long &latitude){				// Converting the Latitude to sth we can read

	if( latitude & 0x4000000 )				//Converted to Signed Integer
    {
        latitude = 0x8000000 - latitude;
        latitude *= -1;
    }
// Lets start converting to Degrees...

   double lat_dd = (int) (latitude / 600000);
		lat_dd += (double) (latitude - (lat_dd * 600000)) / 600000.0;

		 lat_dd = (short) (latitude / 600000);
    double lat_min = abs((latitude - (lat_dd * 600000))) / 10000.0;
char v;
	if (lat_dd>0)
		v='N';
	else
		v='S';

ostringstream latfinal;
latfinal << lat_dd << " " << lat_min << " " << v;
return  latfinal.str();
}
string GetTalkerID(string TalkerID){					// Converting the TalkerID if it is AI, AV etc... Actually may not be nexessary

		//TalkerID = subArray[0].substr(1,2);
if (TalkerID == "AB") TalkerID="NMEA 4.0 Base AIS station";
if (TalkerID == "AD") TalkerID="NMEA 4.0 Dependent AIS station";
if (TalkerID == "AI") TalkerID="Mobile AIS station";
if (TalkerID == "AN") TalkerID="NMEA 4.0 Aid to Navigation AIS station";
if (TalkerID == "AR") TalkerID="NMEA 4.0 AIS Recieving station";
if (TalkerID == "AS") TalkerID="NMEA 4.0 Limited Base station";
if (TalkerID == "AT") TalkerID="NMEA 4.0 AIS Transmitting station";
if (TalkerID == "AX") TalkerID="NMEA 4.0 Repeater AIS station";
if (TalkerID == "BS") TalkerID="Base AIS station";
if (TalkerID == "SA") TalkerID="Physical Shore AIS station";
return TalkerID;
}
