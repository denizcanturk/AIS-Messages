struct NMEA1{									// MsgType 1,2 and 3
unsigned long	MMSI;							//
string			Country;						//
double			Spd;							//
long			longtitude;						//
string			lastlong;						//
string			lastlat;						//
long			latitude;						//
double			GHeading;						// Course Over Ground
double			THeading;						// True Heading
} MsgId_1;
struct NMEA5{									// Complementory Message for formers

unsigned int 	MMSI;							//
string			CallSign;						//
string 			ShipName;

} MsgId_5;
struct NMEA9{									// SAR A/C Message Format...
unsigned int	MMSI;							//
unsigned int	Altitude;						//
int				Spd;							//
long			longtitude;						//
long			latitude;						//
string			lastlong;						//
string			lastlat;						//
double			GHeading;						//

}MsgId_9;

